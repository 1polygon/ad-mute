{
    "manifest_version": 3,
    "name": "Ad Mute",
    "version": "1.0",
    "description": "Mutes the player when ads are playing and skips them when possible. (Twitch, YouTube)",
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.twitch.tv/*"
            ],
            "js": [
                "twitch.js"
            ]
        },
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "youtube.js"
            ]
        }
    ]
}